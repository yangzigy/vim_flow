# .bashrc

# User specific aliases and functions

alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias ..='cd ..'
alias pg='pgrep -l'
k()
{
	if [ $# -eq 1 ]; then 
		kill `pgrep $1`
	fi
}
# Source global definitions
if [ -f /etc/bashrc ]; then
	. /etc/bashrc
fi

#////////////////////////////////////////////
export PS1='\[\033[32m\]\u@\h \[\033[33m\w\033[0m\]
$ '
